{
  "name": "skyway-video-processors-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Workspace root for skyway-video-processors monorepo",
  "scripts": {
    "build": "pnpm --filter skyway-video-processors build",
    "build:examples": "pnpm --filter './examples/*' build",
    "lint": "biome check . --error-on-warnings",
    "lint:fix": "biome check . --write --error-on-warnings",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "husky": "^9.1.7"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
